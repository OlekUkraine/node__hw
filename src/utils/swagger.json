        },
        "age": {
          "type": "integer",
          "minimum": 1,
          "maximum": 150
        },
        "gender": {
          "type": "string",
          "enum": [
            "male",
            "female",
            "other"
          ]
        },
        "phone": {
          "type": "integer"
        },
        "email": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        }
